{
  "googleGeocodeApiKey": "yourkey",
  "googleDirectionsApiKey": null,
  "destination": null,
  "keyCities": [ "mypreferredcity" ],
  "bannedPhrases": [ "badpropertymanagername" ],
  "hideZeros": true,
  "saveFileLocation": "mydesktop",
  "craigslistOptions": {
    "region": "seattle",
    "hasImage": true,
    "postedToday": true,
    "bundleDuplicates": true,
    "includeNearby": false,
    "milesFromZip": null,
    "price": {
      "max": 2200
    },
    "bedrooms": null,
    "sqFt": null,
    "catsOk": false,
    "dogsOk": false,
    "furnished": false,
    "noSmoking": false,
    "wheelchairAccess": false,
    "housingTypes": [
      "CottageCabin",
      "Flat",
      "House",
      "Loft",
      "Apartment"
    ]
  },
  "evaluators": [
    {
      "type": "PriceEvaluator",
      "perfectRange": {
        "Max": 1500,
        "Min": 1000
      },
      "acceptableRange": {
        "Max": 2200,
        "Min": 0
      }
    },
    {
      "type": "BedroomEvaluator",
      "perfectRange": {
        "Max": 2,
        "Min": 2
      },
      "acceptableRange": {
        "Max": 3,
        "Min": 1
      }
    },
    {
      "type": "SquareFootEvaluator",
      "perfectRange": {
        "Max": 1000,
        "Min": 800
      },
      "acceptableRange": {
        "Max": 2500,
        "Min": 1
      }
    },
    {
      "type": "CityEvaluator"
    }
  ]
}